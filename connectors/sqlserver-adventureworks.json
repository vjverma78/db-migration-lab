{
  "name": "sqlserver-adventureworks-connector",
  "config": {
    "connector.class": "io.debezium.connector.sqlserver.SqlServerConnector",
    "tasks.max": "1",

    "database.hostname": "migration-lab-sqlserver",
    "database.port": "1433",
    "database.user": "sa",
    "database.password": "MigrationLab123!",
    "database.names": "AdventureWorksLite",

    "database.server.name": "migrationlab-sqlserver",
    "topic.prefix": "migrationlab-sqlserver",

    "table.include.list": "AdventureWorksLite.dbo.Customers,AdventureWorksLite.dbo.Products,AdventureWorksLite.dbo.Orders,AdventureWorksLite.dbo.OrderItems",

    "snapshot.mode": "initial",
    "include.schema.changes": "false",

    "database.encrypt": "true",
    "database.trustServerCertificate": "true",

    "decimal.handling.mode": "string",
    "time.precision.mode": "adaptive_time_microseconds",

    "tombstones.on.delete": "false",

    "heartbeat.interval.ms": "0",
    "max.batch.size": "2048",
    "max.queue.size": "8192",

    "schema.history.internal.kafka.bootstrap.servers": "migration-lab-kafka:9092",
    "schema.history.internal.kafka.topic": "schema-changes.adventureworks"
  }
}
